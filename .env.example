# Environment Configuration
# Copy this file to .env and fill in your values

# Frontend Configuration (Required for React app)
# =============================================

# Supabase Configuration
VITE_SUPABASE_URL="YOUR_SUPABASE_PROJECT_URL"
VITE_SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_KEY"
VITE_SUPABASE_SERVICE_KEY="YOUR_SUPABASE_SERVICE_KEY" # Service key for bypassing RLS

# LangGraph Configuration for Frontend
# For local development with agent server
VITE_LANGGRAPH_MODE=local
VITE_LANGGRAPH_LOCAL_URL=http://localhost:8123

# For production/cloud deployment (when VITE_LANGGRAPH_MODE is not "local")
# VITE_LANGGRAPH_API_URL=https://your-deployment.us.langgraph.app
# VITE_LANGGRAPH_API_KEY=your-api-key-here

# Agent/Backend Configuration (Required for local agent development)
# =================================================================

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4o-mini  # Optional, defaults to gpt-4o-mini